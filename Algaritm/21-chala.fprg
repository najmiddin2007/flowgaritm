            <assign variable="a" expression="(sqrt*((x2-x1)*(x2-x1))+((y2-y1)*(y2-y1)))"/>
            <assign variable="b" expression="sqrt*((x3-x2)*(x3-x2))+((y3-y2)*(y3-y2))"/>
